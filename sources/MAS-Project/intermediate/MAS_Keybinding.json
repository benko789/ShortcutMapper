{
    "name": "MAS Project",
    "version": "10",
    "default_context": "Global Context",
    "os": ["windows", "mac", "linux"],
    "contexts": {
        "Global Context": {
            "Move Forward": ["W", "W"],
            "Switch Fire Mode": ["Tab+F", "Shift+Ctrl+9"],
            "Test Caps Modifier": ["CAPSLOCK+Q", "Ctrl+Alt+Space"],
            "Test PageUp Modifier": ["Page Up+G", "Shift+Ctrl+6"],
            "Test ` Modifier": ["`+O", "Shift+Ctrl+7"],
            "Toggle Bookmark 4": ["Shift+Ctrl+4", "Shift+Ctrl+4"],
            "Stop": ["Ctrl+F2", "Command+F2"],
            "Toggle Bookmark 5": ["Shift+Ctrl+5", "Shift+Ctrl+5"],
            "Toggle Bookmark 2": ["Shift+Ctrl+2", "Shift+Ctrl+2"],
            "Close Active Tab": ["Shift+Ctrl+F4", "Shift+Ctrl+F4"]
        }
    }
}
